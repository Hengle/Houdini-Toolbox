{
    "path" : ["${HOUDINI_PACKAGE_PATH}/houdini", "${TOOLBOX_HOUDINI_PLUGIN_DIR}"],
    "env": [
        {
            "HOUDINI_TOOLBOX_DIR": "${HOUDINI_PACKAGE_PATH}"
        },
	{
	    "TOOLBOX_HOUDINI_PLUGIN_DIR": "${TOOLBOX_PLUGIN_INSTALL_DIR-${HOUDINI_PACKAGE_PATH}/plugins}/${HOUDINI_VERSION}"
	},
	{
	    "PYTHONPATH":
	    [
		"${TOOLBOX_HOUDINI_PLUGIN_DIR}/python", {
		    "method": "prepend"
		},
		"${HOUDINI_PACKAGE_PATH}/python", {
		    "method": "prepend"
		}
	    ]
	}
    ]
}
